    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^5.1.3",
    "eslint-plugin-deprecation": "~1.3.2",
    "moment": "^2.29.4",
    "ng-recaptcha": "^10.0.0",
    "ngx-markdown": "^14.0.1",
